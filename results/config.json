{
  "data_path": "./data",
  "sensor_types": [
    "attitude",
    "userAcceleration"
  ],
  "activities": [
    "dws",
    "ups",
    "wlk",
    "jog",
    "sit",
    "std"
  ],
  "window_size": 300,
  "stride": 75,
