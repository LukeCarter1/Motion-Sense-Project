    15,
    16,
    17,
    18
  ],
  "test_subjects": [
    20,
    21,
    22,
    23
  ],
  "batch_size": 16,
  "epochs": 40,
  "learning_rate": 0.001,
  "weight_decay": 1e-05,
  "dropout_rate": 0.5,
  "model_name": "cnn_hybrid",
